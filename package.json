    "@nestjs/schedule": "^2.0.0 || ^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "ftp": "^0.3.10",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.0.0",
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
