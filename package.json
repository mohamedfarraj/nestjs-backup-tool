  "description": "A NestJS backup module for databases with multiple storage options",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
